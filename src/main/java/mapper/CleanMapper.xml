<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace:理解成生成的实现类的全路径(包名+类名) -->
<!--suppress ALL -->
<mapper namespace="clean">
    <delete id="deleteFile">
        delete
        from t_file
    </delete>
    <delete id="deleteRule">
        delete
        from t_rule
    </delete>
    <delete id="deleteProvenance">
        delete
        from t_provenance
    </delete>
</mapper>